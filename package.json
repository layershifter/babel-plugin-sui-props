{
  "name": "babel-plugin-transform-react-handled-props",
  "version": "0.2.5",
  "description": "Generates handledProps from defaultProps and propTypes during the build",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "babel-types": "^6.25.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "ta-scripts": "^2.5.2"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "release:major": "npm run prerelease && ta-script npm/release.sh major",
    "release:minor": "npm run prerelease && ta-script npm/release.sh minor",
    "release:patch": "npm run prerelease && ta-script npm/release.sh patch",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "prerelease": "npm run clean && npm run build"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "react",
    "defaultProps",
    "handledProps",
    "propTypes"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/layershifter/babel-plugin-transform-react-handled-props.git"
  },
  "author": "Alexander Fedyashov <a@fedyashov.com>",
  "homepage": "https://github.com/layershifter/babel-plugin-transform-react-handled-props",
  "license": "MIT"
}
