{
  "name": "babel-plugin-transform-react-handled-props",
  "version": "0.2.5",
  "description": "Generates handledProps from defaultProps and propTypes during the build",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "babel-types": "^6.26.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.6.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "mocha": "^4.0.0",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "ta-scripts": "^2.5.2"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib",
    "lint": "prettier --list-different \"src/**/*.js\" \"test/test.js\" && eslint \"src/**/*.js\" \"test/test.js\"",
    "lint:fix": "prettier --write \"src/**/*.js\" \"test/test.js\" && eslint --fix \"src/**/*.js\" \"test/test.js\"",
    "prerelease": "npm run clean && npm run build",
    "release:major": "npm run prerelease && ta-script npm/release.sh major",
    "release:minor": "npm run prerelease && ta-script npm/release.sh minor",
    "release:patch": "npm run prerelease && ta-script npm/release.sh patch",
    "test": "mocha --require babel-register \"test/*.js\"",
    "test:watch": "npm run test -- --watch --watch-extensions js"
  },
  "keywords": [
    "babel",
    "babel-plugin",
    "react",
    "defaultProps",
    "handledProps",
    "propTypes"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/layershifter/babel-plugin-transform-react-handled-props.git"
  },
  "author": "Alexander Fedyashov <a@fedyashov.com>",
  "homepage": "https://github.com/layershifter/babel-plugin-transform-react-handled-props",
  "license": "MIT"
}
